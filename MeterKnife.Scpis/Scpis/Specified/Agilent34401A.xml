<?xml version="1.0" encoding="utf-8"?>

<instrument>
  <information version="1.0" brand="Agilent" name="34401A" type="DigitalMultimeter">
  </information>
  <scpigroups>
    <subject description="电压测量">
      <group way="init">
        <scpi interval="400">
          <![CDATA[*CLS]]>
        </scpi>
        <scpi interval="400">
          <![CDATA[CONF:VOLT:DC]]>
        </scpi>
      </group>
      <group way="collect">
        <scpi interval="400" return="true">
          <![CDATA[READ?]]>
        </scpi>
      </group>
    </subject>
    <subject description="电流测量">
      <group way="init">
        <scpi interval="400">
          <![CDATA[*CLS]]>
        </scpi>
        <scpi interval="400">
          <![CDATA[CONF:CURR:DC]]>
        </scpi>
      </group>
      <group way="collect">
        <scpi interval="400" return="true">
          <![CDATA[READ?]]>
        </scpi>
      </group>
    </subject>
    <subject description="电阻测量">
      <group way="init">
        <scpi interval="400">
          <![CDATA[*CLS]]>
        </scpi>
        <scpi interval="400">
          <![CDATA[CONF:RES]]>
        </scpi>
      </group>
      <group way="collect">
        <scpi interval="400" return="true">
          <![CDATA[READ?]]>
        </scpi>
      </group>
    </subject>
    <subject description="四线电阻测量">
      <group way="init">
        <scpi interval="400">
          <![CDATA[*CLS]]>
        </scpi>
        <scpi interval="400">
          <![CDATA[CONF:FRES]]>
        </scpi>
      </group>
      <group way="collect">
        <scpi interval="400" return="true">
          <![CDATA[READ?]]>
        </scpi>
      </group>
    </subject>
  </scpigroups>
</instrument>