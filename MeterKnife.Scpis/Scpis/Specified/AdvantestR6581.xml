<?xml version="1.0" encoding="utf-8"?>

<instrument>
  <information version="1.0" brand="Advantest" name="R6581" description="-Advantest-R6581" type="DigitalMultimeter">
  </information>
  <scpigroups>
    <subject description="电压测量">
      <group way="init">
        <scpi interval="1000">
          <![CDATA[*CLS]]>
        </scpi>
        <scpi interval="1000">
          <![CDATA[CONF:VOLT:DC]]>
        </scpi>
        <scpi interval="1000">
          <![CDATA[SENS:VOLT:DC:DIG 7]]>
        </scpi>
        <scpi interval="1000">
          <![CDATA[SENS:VOLT:DC:DIG 8]]>
        </scpi>
        <scpi interval="1000">
          <![CDATA[SENS:VOLT:DC:NPLC 10]]>
        </scpi>
        <scpi interval="1000">
          <![CDATA[SENS:VOLT:DC:NPLC 20]]>
        </scpi>
      </group>
      <group way="collect">
        <scpi interval="10000" return="true">
          <![CDATA[READ?]]>
        </scpi>
      </group>
    </subject>
    <subject description="电流测量">
      <group way="init">
        <scpi interval="1000">
          <![CDATA[*CLS]]>
        </scpi>
        <scpi interval="1000">
          <![CDATA[CONF:CURR:DC]]>
        </scpi>
      </group>
      <group way="collect">
        <scpi interval="10000" return="true">
          <![CDATA[READ?]]>
        </scpi>
      </group>
    </subject>
    <subject description="电阻测量">
      <group way="init">
        <scpi interval="1000">
          <![CDATA[*CLS]]>
        </scpi>
        <scpi interval="1000">
          <![CDATA[CONF:RES]]>
        </scpi>
      </group>
      <group way="collect">
        <scpi interval="10000" return="true">
          <![CDATA[READ?]]>
        </scpi>
      </group>
    </subject>
    <subject description="四线电阻测量">
      <group way="init">
        <scpi interval="1000">
          <![CDATA[*CLS]]>
        </scpi>
        <scpi interval="1000">
          <![CDATA[CONF:FRES]]>
        </scpi>
      </group>
      <group way="collect">
        <scpi interval="10000" return="true">
          <![CDATA[READ?]]>
        </scpi>
      </group>
    </subject>
  </scpigroups>
</instrument>